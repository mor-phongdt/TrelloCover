line hide-details></v-text-field>\n      <v-spacer></v-spacer>\n      <div class=\"d-flex justify-end\">\n        <v-btn\n          class=\"mr-4\"\n          width=\"100%\"\n          large\n          color=\"secondary\n        \"\n          to=\"/song/create\"\n          >Add</v-btn\n        >\n      </div>\n    </v-card-title>\n    <v-data-table\n      :headers=\"headers\"\n      :items=\"songs\"\n      :search=\"search\"\n      class=\"elevation-4 mt-5\"\n      show-select\n      v-model=\"selected\"\n    >\n      <template v-slot:item.name=\"{ item }\">\n        <p style=\"word-break: break-word;\">{{ item.name }}</p>\n      </template>\n      <template v-slot:item.singer=\"{ item }\">\n        <p v-for=\"item in item.singer\" :key=\"item.id\">{{ item }}</p>\n      </template>\n      <template v-slot:item.category=\"{ item }\">\n        <p style=\"word-break: break-word;\">{{ item.category }}</p>\n      </template>\n      <template v-slot:item.lyrics=\"{ item }\">\n        <p style=\"word-break: break-word;\" v-if=\"item.lyrics.length < 100\">{{ item.lyrics }}</p>\n        <p style=\"word-break: break-word;\" v-else>{{ item.lyrics.substring(0, 100) + \"...\" }}</p>\n      </template>\n      <template v-slot:item.imageFile=\"{ item }\" style=\"mt-5\">\n        <v-img :src=\"item.imageFile\" alt max-width=\"100\" max-height=\"100\" />\n      </template>\n      <template v-slot:item.released=\"{ item }\">\n        <p style=\"word-break: break-word;\">{{ item.released }}</p>\n      </template>\n      <template v-slot:item.action=\"{ item }\">\n        <v-icon small class=\"mr-2\" @click=\"edit(item)\">mdi-pencil</v-icon>\n        <v-icon small @click=\"deleteItem(item)\">mdi-delete</v-icon>\n      </template>\n    </v-data-table>\n  </div>\n</template>\n\n<script>\nimport { mapGetters, mapActions, mapMutations } from 'vuex';\n\nexport default {\n  data() {\n    return {\n      search: '',\n      selected: [],\n      headers: [\n        {\n          text: 'Name', align: 'left', value: 'name', width: '15%',\n        },\n        { text: 'Singer', value: 'singer', width: '15%' },\n        { text: 'Category', value: 'category' },\n        { text: 'Lyrics', value: 'lyrics', class: 'break-long-text' },\n        { text: 'Image', value: 'imageFile' },\n        { text: 'Released', value: 'released' },\n        { text: 'action', sortable: false, value: 'action' },\n      ],\n      songs: [\n        {\n          name: 'Senorita',\n          singer: ['Shawn Medes', 'Camila Cabello'],\n          category: 'nhac ngoai',\n          lyrics:\n            'pppppppppppppppppppppppppppppppppppppppppppppppppppppppppppppppppppppppppppppppppppppppppppppppppppppp',\n          imageFile:\n            'https://media.ex-cdn.com/EXP/media.giadinhmoi.vn/files/content/2019/06/22/senorita-03-1704.jpg',\n          released: '20/9/2019',\n        },\n      ],\n      drawer: true,\n    };\n  },\n  created() {\n    const loader = this.$loading.show();\n    this.getSongList()\n      .then(() => {\n        loader.hide();\n      })\n      .catch((error) => {\n        this.$store.commit('setSnack', {\n          snack: `Something went wrong, please try again later${error.message}`,\n          color: 'error',\n        });\n        loader.hide();\n      });\n    this.$store.watch(\n      state => state.hambuger.drawer,\n      () => {\n        const msg = this.$store.state.hambuger.drawer;\n        if (msg !== '') {\n          this.drawer = msg;\n        }\n      },\n    );\n  },\n  computed: {\n    ...mapGetters('song', {\n      songs: 'renderSongList',\n    }),\n  },\n  methods: {\n    ...mapActions('song', {\n      getSongList: 'getSongList',\n    }),\n    deleteItem(item) {\n      const message = 'Do you want delete';\n      this.$store.commit('showDialog', {\n        message,\n        submitFunction: 'removeUser',\n        data: item,\n      });\n    },\n    edit(item) {\n      this.$router.push({\n        name: 'SongEditPage',\n        params: { id: item.id },\n      });\n    },\n  },\n};\n</script>\n"]}]}