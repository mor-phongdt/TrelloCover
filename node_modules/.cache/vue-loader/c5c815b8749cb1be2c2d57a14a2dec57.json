r>\n      <div class=\"d-flex justify-end\">\n        <v-btn class=\"mr-4\" width=\"100%\" x-large color=\"#40b883\" to=\"/singer/create\">Add</v-btn>\n      </div>\n    </v-card-title>\n    <v-data-table :headers=\"headers\" :items=\"singers\" :search=\"search\" class=\"elevation-4\">\n      <template v-slot:item.fullname=\"{ item }\">\n        <p style=\"word-break: break-word;\">{{item.fullname}}</p>\n      </template>\n      <template v-slot:item.birthday=\"{ item }\">\n        <p style=\"word-break: break-word;\">{{item.birthday}}</p>\n      </template>\n      <template v-slot:item.imageFile=\"{ item }\" style=\"mt-5\">\n        <v-img :src=\"item.imageFile\" alt max-width=\"200\" max-height=\"200\" />\n      </template>\n      <template v-slot:item.followers=\"{ item }\">\n        <p style=\"word-break: break-word;\">{{item.followers}}</p>\n      </template>\n      <template v-slot:item.country=\"{ item }\">\n        <p style=\"word-break: break-word;\">{{item.country}}</p>\n      </template>\n      <template v-slot:item.song=\"{ item }\">\n        <p v-for=\"item in item.song\" :key=\"item.id\">{{item}}</p>\n      </template>\n      <template v-slot:item.biography=\"{ item }\">\n        <p style=\"word-break: break-word;\" v-if=\"item.biography.length<65\">{{item.biography}}</p>\n        <p style=\"word-break: break-word;\" v-else>{{item.biography.substring(0,65) + \"...\"}}</p>\n      </template>\n      <template v-slot:item.action=\"{ item }\">\n        <v-icon small class=\"mr-2\" @click=\"edit(item)\">mdi-pencil</v-icon>\n        <v-icon small @click=\"deleteItem(item)\">mdi-delete</v-icon>\n      </template>\n    </v-data-table>\n  </div>\n</template>\n\n<script>\nimport { mapGetters, mapActions, mapMutations } from \"vuex\";\n\nexport default {\n  data() {\n    return {\n      search: \"\",\n      headers: [\n        {\n          text: \"Fullname\",\n          align: \"center\",\n          value: \"fullname\",\n          width: \"12%\"\n        },\n        { text: \"Birthday\", value: \"birthday\", width: \"10%\", sortable: false ,align: \"center\", },\n        { text: \"Image\", value: \"imageFile\", width: \"15%\", sortable: false ,align: \"center\" },\n        { text: \"Followers\", value: \"followers\", width: \"2%\", sortable: false ,align: \"center\" },\n        {\n          text: \"Country\",\n          value: \"country\",\n          class: \"break-long-text\",\n          width: \"6%\",\n          sortable: false,\n          align: \"center\"\n        },\n        { text: \"Song\", value: \"song\", sortable: false ,align: \"center\"},\n        {\n          text: \"Biography\",\n          value: \"biography\",\n          width: \"20%\",\n          sortable: false,\n          align: \"center\"\n        },\n        { text: \"action\", sortable: false, value: \"action\", sortable: false ,align: \"center\"}\n      ],\n      singers: [\n        {\n          id: 1,\n          fullname: \"dang tuan phong\",\n          birthday: \"13/11/1998\",\n          imageFile:\n            \"https://image.shutterstock.com/image-photo/beautiful-water-drop-on-dandelion-260nw-789676552.jpg\",\n          followers:20,\n          country: \"Hung Yen\",\n          song: [\"hay trao cho anh\", \"em cua ngay hom qua\"],\n          biography:\n            \"fasfasfasfddddddddddddddfdsfsdfsddsfsdfsdddddddddddddddddddddddddddddddddddddddddddddddddddddddddddddddddddddddddddddddddddddddd\"\n        }\n      ],\n      drawer: true\n    };\n  },\n  created() {\n    let loader = this.$loading.show();\n    // console.log(window.location.pathname);\n    // console.log(typeof(window.location.pathname))\n    this.getSingerList()\n    .then(()=>{\n      loader.hide();\n    })\n    .catch((error)=>{\n      // this.$store.commit('setSnack', { snack: `Something went wrong, please try again later${error.message}`, color: 'error' });\n      loader.hide();\n    })\n    this.$store.watch(\n      state => state.hambuger.drawer,\n      () => {\n        const msg = this.$store.state.hambuger.drawer;\n        if (msg !== \"\") {\n          this.drawer = msg;\n        }\n      }\n    );\n  },\n  computed: {\n    ...mapGetters('singer',{\n      singers: 'renderSingerList'\n    }),\n  },\n  methods: {\n    ...mapActions('singer',{\n      getSingerList : 'getSingerList'\n    }),\n    deleteItem(item) {\n      const message = \"Do you want delete\";\n      this.$store.commit(\"showDialog\", {\n        message,\n        submitFunction: \"removeUser\",\n        data: item\n      });\n    },\n    edit(item) {\n      this.$router.push({\n        name: \"SingerEditPage\",\n        params: { id: item.id }\n      });\n    }\n  }\n};\n</script>\n<style scoped>\n.mr-4 {\n  color: #435b71;\n}\n</style>\n"]}]}