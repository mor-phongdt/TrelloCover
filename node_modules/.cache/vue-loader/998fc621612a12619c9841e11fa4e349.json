   </div>\n        </template>\n    </v-navigation-drawer>\n  </v-app>\n</template>\n\n<script>\nimport { mapActions } from 'vuex';\n\nexport default {\n  data() {\n    return {\n      drawer: true,\n      items: [\n        { title: 'Artist', icon: 'mdi-artist', route: '/singer' },\n        { title: 'Song', icon: 'mdi-library-music', route: '/song' },\n      ],\n      snackbar: false,\n    };\n  },\n  watch: {\n    drawer() {\n      this.$store.commit('setDrawer', { status: this.drawer });\n    },\n  },\n  methods: {\n    ...mapActions('auth', {\n      logout: 'logout',\n    }),\n    logoutEvent() {\n      this.logout()\n        .then(() => {\n          this.$router.push({\n            name: 'loginpage',\n          })\n            .catch((error) => {\n              this.$store.commit('setSnack', {\n                snack: `Something went wrong, please try again later${error.message}`,\n                color: 'error',\n              });\n            });\n        });\n    },\n  },\n};\n</script>\n"]}]}